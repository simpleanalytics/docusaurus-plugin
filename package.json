{
  "name": "docusaurus-plugin-simple-analytics",
  "version": "1.1.8",
  "description": "Simple Analytics plugin for Docusaurus.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!lib/.tsbuildinfo"
  ],
  "keywords": [
    "docusaurus",
    "analytics"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc --build --watch",
    "postinstall": "cd node_modules/docusaurus-plugin-simple-analytics && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simpleanalytics/docusaurus-plugin.git"
  },
  "license": "MIT",
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/types": "^2.4.1",
    "@docusaurus/utils-validation": "^2.4.1"
  },
  "devDependencies": {
    "tslib": "^2.5.2",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "react": "^16.8.4 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.4 || ^17.0.0 || ^18.0.0"
  },
  "engines": {
    "node": ">=16.14"
  }
}
